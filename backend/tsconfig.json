{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "skipLibCheck": true
  }
}